<h1>Update Shelter</h1>
<%= form_tag "/shelters/#{@shelter.id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name, "Name" %>
  <%= label_tag :address %>
  <%= text_field_tag :address, "Address" %>
  <%= label_tag :city %>
  <%= text_field_tag :city, "City" %>
  <%= label_tag :state %>
  <%= text_field_tag :state, "State" %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip, "Zip" %>
  <%= submit_tag "Submit" %>
<% end %>
